{
  "name": "Electron3",
  "version": "3.0.0",
  "license": "ISC",
  "description": "My Electron3 description",
  "author": "AlexHu",
  "productName": "Electron3",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-builder install-app-deps",
    "start": "npm install && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HuAlex007/Electron3.git"
  },
  "bugs": {
    "url": "https://github.com/HuAlex007/Electron3/issues"
  },
  "homepage": "https://github.com/HuAlex007/Electron3#readme",
  "devDependencies": {
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "electron-winstaller": "^4.0.0"
  },
  "dependencies": {
    "electron-builder-squirrel-windows": "^22.5.1",
    "electron-log": "^4.1.1",
    "update-electron-app": "^1.5.0"
  }
}
